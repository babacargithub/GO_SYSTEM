
  <header class="main-header">
    <!-- Logo -->
    <a href="/" class="logo">
      <!-- mini logo for sidebar mini 50x50 pixels -->
      <span class="logo-mini"><b>Golob</b>One</span>
      <!-- logo for regular state and mobile devices -->
      <span class="logo-lg"><b>GO</b>{% block app_name %}SYSTEM{% endblock app_name %}</span>
    </a>
    <!-- Header Navbar: style can be found in header.less -->
    <nav class="navbar navbar-static-top">
      <!-- Sidebar toggle button-->
      <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
        <span class="sr-only">Toggle navigation</span>
      </a>

      <div class="navbar-custom-menu pull-left">
        <ul class="nav navbar-nav">
          <!-- Messages: style can be found in dropdown.less-->
          {% for menuItem in menuItems %}
          <li class="dropdown messages-menu">
            <a href="{{menuItem.href}}" class="dropdown-togglek" >
              <i class="fa fa-folder-open"></i>
             {{menuItem.libelle}}
            </a>
          </li>
          {%endfor %}
        </ul>
      </div>
      <div class="navbar-custom-menu pull-right">
          <!-- User Account: style can be found in dropdown.less -->
          <ul class="nav navbar-nav">
          <li class="dropdown user user-menu">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <img src="{{asset("admin-lte/dist/img/user2-160x160.jpg")}}" class="user-image" alt="User Image">
              <span class="hidden-xs">{% if app.user%}{{app.user.prenom~" "~app.user.nom|upper}} {%endif%}</span>
               {% if app.session.get('connected_shop') is defined and app.session.get('connected_shop') is not null %}
                   {{ app.session.get('connected_shop').libelle}}
                   {%endif %}
            </a>
            <ul class="dropdown-menu">
              <!-- User image -->
              <li class="user-header">
                <img src="{{asset("admin-lte/dist/img/avatar.png")}}" class="img-circle" alt="User Image">

                <p>
                  {% if app.user%}{{app.user.prenom~" "~app.user.nom|upper}} {%endif%}-Poste Employé
                  <small>Depuis 2013</small>
                </p>
              </li>
              <!-- Menu Body -->
              <li class="user-body">
                <div class="row">
                  <div class="col-xs-3 text-center">
                    <a href="{{path("go_main_app_authentication",{"app":"caravane"})}}">Caravane</a>
                  </div>
                  <div class="col-xs-3 text-center">
                    <a href="{{path("go_main_app_authentication",{"app":"shop"})}}">Shop</a>
                  </div>
                  <div class="col-xs-3 text-center">
                    <a href="{{path("go_main_app_authentication",{"app":"caisse_pro"})}}">Caisse Pro</a>
                  </div>
                  <div class="col-xs-3 text-center">
                    <a href="{{path("go_main_app_authentication",{"app":"admin"})}}">Admin</a>
                  </div>
                  
                </div>
                <div class="row">
                  <div class="col-xs-12 text-center">
                      <a href="#">Sélectionner Application</a>
                   </div>
                  
                </div>
                <!-- /.row -->
              </li>
              <!-- Menu Footer-->
              <li class="user-footer">
			  <div class="pull-left">
                  <a href="{{path("fos_user_security_logout")}}" class="btn btn-default btn-flat">Déconnexion</a>
                </div>
                <div class="pull-right">
                  <a href="" class="btn btn-default btn-flat">Mon Compte</a>
                </div>
                
              </li>
            </ul>
          </li>
          <!-- Control Sidebar Toggle Button -->
          <li>
            <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
          </li>
        </ul>
      </div>
    </nav>
  </header>