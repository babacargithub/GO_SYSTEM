{% extends 'base.html.twig' %}

{% block content_body %}
    <h1>Typecompte</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ typeCompte.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ typeCompte.name }}</td>
            </tr>
            <tr>
                <th>Comments</th>
                <td>{{ typeCompte.comments }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('type_compte_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('type_compte_edit', { 'id': typeCompte.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
