{% if form is defined %}
<form id="{{form.vars.name}}"action="{{path('go_shop_produit_update')}}" class="form-horizontal" method="post" {{ form_enctype(form) }}>
<div>
    {{ form_errors(form)}}
</div>
    
    
    
    {{ form_widget(form.id,{"attr":{"hidden":true}})}}
    
    <div>
    {{ form_label(form.nom, "Libellé")}}
    {{ form_widget(form.nom)}}
    </div>
    <div>
    {{ form_label(form.type, "Type de produit")}}
    {{ form_widget(form.type,{"attr":{"class":"form-control"}} )}}
    </div>
    <div>
    {{ form_label(form.prixAchat, "Prix Achat")}}
    {{ form_widget(form.prixAchat )}}
    </div>
    <div>
    {{ form_label(form.prixVente, "Prix Vente")}}
    {{ form_widget(form.prixVente,{"attr":{"class":"form-control"}} )}}
    </div>
    <div>
    {{ form_label(form.descrip, "Description")}}
    {{ form_widget(form.descrip,{"attr":{"class":"form-control"}} )}}
    </div>
    <div>
    {{ form_label(form.categorie, "Catégorie")}}
    {{ form_widget(form.categorie,{"attr":{"class":"form-control"}} )}}
    </div>
   
        {{ form_row(form._token) }}
     {# {{ form_widget(form)}}#}
<input type="submit" class="btn btn-primary" />
</form>
</div>

    {% endif %}

  