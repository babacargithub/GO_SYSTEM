<?php

namespace  GO\ShopBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * RembCreance
 *
 * @ORM\Table(name="remb_creance_liquide")
 * repositoryClass="GO\ShopBundle\Entity\RembCreanceLiquideRepository")
 */
//==============Entité représentant les remboursements des créances 
class RembCreanceLiquide extends RembCreance
{
    /**
     * @ORM\ManyToOne(targetEntity="GO\ShopBundle\Entity\CreanceLiquide", inversedBy="remboursements")
     * @ORM\JoinColumns({@ORM\JoinColumn(name="creance", referencedColumnName="id", nullable=false)})
     */
    private $creance;

    /**
     * Set creance
     *
     * @param \GO\ShopBundle\Entity\Creance $creance
     * @return RembCreance
     */
    public function setCreance(\GO\ShopBundle\Entity\CreanceLiquide $creance)
    {
        $this->creance = $creance;

        return $this;
    }

    /**
     * Get creance
     *
     * @return \GO\ShopBundle\Entity\Creance 
     */
    public function getCreance()
    {
        return $this->creance;
    }
}
