{% extends "@GOCaisse/entree/main_layout.html.twig"%}
{% block content_body %}       
{% if form is defined %}
    <div class="col-lg-6">
    <form action="{{ path('caisse_pro_entree_search')}}" method="post">
           {{ form_errors(form)}}
            {{ form_widget(form)}} 
    
